{
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "fix": "yarn fix:eslint && yarn fix:prettier && yarn fix:stylelint",
    "fix:eslint": "eslint **/*.{js,ts} --fix",
    "fix:prettier": "prettier --write **/*.{js,ts,css,sass,scss}",
    "fix:stylelint": "stylelint --fix **/*.{css,sass,scss} --ignore-path",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:eslint": "eslint **/*.{js,ts}",
    "lint:prettier": "prettier --check **/*.{js,ts,css,sass,scss}",
    "lint:stylelint": "stylelint **/*.{css,sass,scss} --ignore-path"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": "eslint --fix",
    "src/**/*.(css|sass|scss)": "stylelint --fix"
  },
